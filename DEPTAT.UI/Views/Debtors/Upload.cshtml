@model DEPTAT.Application.Responses.BaseResponse<DebtorsResponse>
<div class="row clearfix">
    <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
        <div class="card">
            <div class="header">
                <h2>
                    UPLOAD RESULT SUMMARY
                </h2>
                <ul class="header-dropdown m-r--5">
                    <a asp-controller="Debtors" asp-action="Index" class="btn btn-primary"> Back</a>
                </ul>
            </div>
            <div class="body">
                
                
                @if (Model.IsSuccess)
                {
                    <p style="font-weight: bold;">@Model.Message</p>
                    @foreach (var data in Model.Errors)
                    {
                        <div class="alert bg-green alert-dismissible" role="alert">
                            <button type="button" class="close" data-dismiss="alert" aria-label="Close"><span aria-hidden="true">×</span></button>
                            @data
                        </div>
                    }
                }
                else
                {
                    <div class="alert bg-red alert-dismissible" role="alert">
                        <button type="button" class="close" data-dismiss="alert" aria-label="Close"><span aria-hidden="true">×</span></button>
                        @Model.Message
                    </div>
                    @foreach (var data in Model.Errors)
                    {
                        <div class="alert bg-red alert-dismissible" role="alert">
                            <button type="button" class="close" data-dismiss="alert" aria-label="Close"><span aria-hidden="true">×</span></button>
                            @data
                        </div>
                    }
                }
            </div>
        </div>
    </div>
</div>

                    
                    
